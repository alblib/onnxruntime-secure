{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Install Build Tools",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/1_install_build_tools.py",
      "console": "integratedTerminal",
      "args": [
        // for a positional "root" arg:
        "--install-android-sdk",
        "--install-web-sdk"
      ]
    },
    {
      "name": "Download API Sources",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/2_download_sources.py",
      "console": "integratedTerminal",
      "args": [
        // for a positional "root" arg:
        "${workspaceFolder}",
        "opencl",
        "onnxruntime"
      ]
    },
    {
      "name": "Install ONNX Runtime Windows",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/3_install_onnxruntime_windows.py",
      "console": "integratedTerminal",
      "args": [
        // for a positional "root" arg:
        "${workspaceFolder}",
        "--arch", "x64"
      ]
    },
    {
      "name": "Install ONNX Runtime Android",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/3_install_onnxruntime_android.py",
      "console": "integratedTerminal",
      "args": [
        // for a positional "root" arg:
        "${workspaceFolder}",
        "--nnapi",
        "--arch", "armeabi-v7a"
      ]
    },
    {
      "name": "Install ONNX Runtime WebAssembly",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/3_install_onnxruntime_web.py",
      "console": "integratedTerminal",
      "args": [
        // for a positional "root" arg:
        "${workspaceFolder}"
      ]
    }
  ]
}
