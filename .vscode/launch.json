{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Download Android SDK",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/2_download_android_sdk.py",
      "console": "integratedTerminal",
      "args": [
        // for a positional "root" arg:
        "${workspaceFolder}",
      ]
    },
    {
      "name": "Download API Sources",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/2_download_sources.py",
      "console": "integratedTerminal",
      "args": [
        // for a positional "root" arg:
        "${workspaceFolder}",
        "opencl",
        "onnxruntime"
      ]
    },
    {
      "name": "Install ONNX Runtime DML",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/3_install_onnxruntime_dml.py",
      "console": "integratedTerminal",
      "args": [
        // for a positional "root" arg:
        "${workspaceFolder}"
      ]
    }
  ]
}
